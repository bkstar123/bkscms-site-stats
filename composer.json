{
    "name": "bkstar123/bkscms-site-stats",
    "description": "A bkstar123/bkscms's package visualizes some basic Google Analytics data for a website from a **BKSCMS** CMS project",
    "keywords": [
        "laravel",
        "google analytics",
        "site statistics",
        "page views",
        "new visitors",
        "active users",
        "bounce rate",
        "returning visitors"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hoang Anh Tuan",
            "email": "hoangtuandhbk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "spatie/laravel-analytics": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Bkstar123\\BksCMS\\SiteStats\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bkstar123\\BksCMS\\SiteStats\\Providers\\SiteStatsServiceProvider"
            ]
        }
    }
}
